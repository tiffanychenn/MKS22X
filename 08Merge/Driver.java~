import java.util.*;

public class Driver{

    public static void main(String[] args){
	int[] ary1 = makeRandomizedArray(args[0]);
	int[] ary2 = makeDuplicateArray(args[0]);
	int[] ary3 = makeSameArray(args[0]);
	int[] ary4 = makeSortedArray(args[0]);
	int[] ary5 = makeReverseArray(args[0]);
    }

    public static int[] makeRandomizedArray(String s){
	int a = Integer.parseInt(s);
	Random rand = new Random();
	int[] temp = new int[s];
	for (int i = 0; i < a; i ++){
	    temp[i] = rand.nextInt();
	}
	return temp;
    }

    public static int[] makeDuplicateArray(String s){
	int a = Integer.parseInt(s);
	Random rand = new Random();
	int[] temp = new int[s];
	for (int i = 0; i < a; i ++){
	    temp[i] = rand.nextInt(10);
	}
	return temp;
    }

    public static int[] makeRandomizedArray(String s){
	int a = Integer.parseInt(s);
	Random rand = new Random();
	int[] temp = new int[s];
	for (int i = 0; i < a; i ++){
	    temp[i] = rand.nextInt();
	}
	return temp;
    }

    public static int[] makeRandomizedArray(String s){
	int a = Integer.parseInt(s);
	Random rand = new Random();
	int[] temp = new int[s];
	for (int i = 0; i < a; i ++){
	    temp[i] = rand.nextInt();
	}
	return temp;
    }

    public static int[] makeRandomizedArray(String s){
	int a = Integer.parseInt(s);
	Random rand = new Random();
	int[] temp = new int[s];
	for (int i = 0; i < a; i ++){
	    temp[i] = rand.nextInt();
	}
	return temp;
    }

}
